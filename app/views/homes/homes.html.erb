<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

<div class="home-image">
</div>

<div class="home-page">
  <% if logged_in? %>
    <div class="display-buttons">
      <%= link_to "Teachers", teachers_path %> | <%= link_to "Students", students_path %>
    </div>
  <% else %>
    <div class="login-form">
    <%= render 'sessions/new' %>
    </div>
  <% end %>
</div>


